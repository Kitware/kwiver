#
# Precache settings for building fletch
#
# Selects required modules
#
(set fletch_ENABLE_ALL_PACKAGES                   OFF)
(set fletch_ENABLE_Boost                          ON)
(set fletch_ENABLE_Ceres                          ON)
(set fletch_ENABLE_Eigen                          ON)
(set fletch_ENABLE_OpenCV                         ON)
(set fletch_ENABLE_PNG                            ON)
(set fletch_ENABLE_SparseSuite                    ON)
(set fletch_ENABLE_TinyXML                        ON)
(set fletch_ENABLE_Zlib                           ON)
(set fletch_ENABLE_libjpeg-turbo                  ON)
(set fletch_ENABLE_libtiff                        ON)
(set FLETCH_BUILD_WITH_PYTHON                     ON)
